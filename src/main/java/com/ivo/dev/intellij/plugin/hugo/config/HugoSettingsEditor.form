<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.ivo.dev.intellij.plugin.hugo.config.HugoSettingsEditor">
  <grid id="27dc6" binding="settingsPanel" layout-manager="GridLayoutManager" row-count="3" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="0" left="0" bottom="0" right="0"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="c5f36" class="javax.swing.JCheckBox" binding="pathToHugoExecutableCheckBox" default-binding="true">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Path to Hugo executable: "/>
        </properties>
      </component>
      <vspacer id="cb8bc">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
        </constraints>
      </vspacer>
      <component id="65dba" class="com.intellij.openapi.ui.TextFieldWithBrowseButton" binding="customHugoPathField">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="3" hsize-policy="7" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <editable value="true"/>
          <enabled value="true"/>
        </properties>
      </component>
      <component id="8314f" class="javax.swing.JLabel">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <foreground color="-8224126"/>
          <text value="Manually specify Hugo location if it was not added to PATH"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
